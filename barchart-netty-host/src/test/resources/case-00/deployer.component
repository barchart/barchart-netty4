#
# example of stand-alone component
#

# sequence writer
{

	# dot factory id
	type = barchart.netty.dot.cast
	 
	# guid
	id = writer-001 
	 
	# pipe builder name
	pipeline = barchart.netty.pipe.sequence.writer
	 
	# bind address
	local-address = "localhost:0"
	 
	# write address
	remote-address = "239.1.2.3:12345"
	 
	# time to live
	packet-ttl = 7
         
}
